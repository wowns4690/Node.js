<!DOCTYPE html>
<html>
    <body>
        <table border="1" >
            <tr>
                <th>번호</th>
                <th>제목</th>
                <th>작성자</th>
            </tr>
            <%
                for(var i=0; i<data.length; i++){
            %>
            <tr>
                <td align="center"><%= data[i].boardNum%></td>
                <td align="center"><a href="board/show/<%= data[i].boardNum%>"><%= data[i].Title%></a></td>
                <td align="center"><%= data[i].Writer%></td>
                <td colspan="2"><button onclick="Delete('<%= data[i].boardNum%>')">삭제</button><button onclick="location.href='board/edit/<%= data[i].boardNum%>'">수정</button></td>
            </tr>
            <%}%>
        </table>
        <button onclick="location.href='board/new'">글 작성</button>
    </body>
    <script src="/javascripts/board.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
